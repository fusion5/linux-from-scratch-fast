wget http://ftp.gnu.org/gnu/gcc/gcc-9.2.0/gcc-9.2.0.tar.xz -O gcc-9.2.0.tar.xz
tar -xvf gcc-9.2.0.tar.xz
wget http://ftp.gnu.org/gnu/gmp/gmp-6.1.2.tar.xz -O gmp-6.1.2.tar.xz 
tar -xvf gmp-6.1.2.tar.xz
wget http://www.mpfr.org/mpfr-4.0.2/mpfr-4.0.2.tar.xz -O mpfr-4.0.2.tar.xz
tar -xvf mpfr-4.0.2.tar.xz
wget https://ftp.gnu.org/gnu/mpc/mpc-1.1.0.tar.gz -O mpc-1.1.0.tar.gz
tar -xvf mpc-1.1.0.tar.gz

mv gmp-6.1.2  gcc-9.2.0/gmp
mv mpfr-4.0.2 gcc-9.2.0/mpfr
mv mpc-1.1.0  gcc-9.2.0/mpc
